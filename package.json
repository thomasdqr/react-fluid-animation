{
  "name": "react-fluid-animation",
  "version": "1.0.2",
  "description": "Fluid media animation for React powered by WebGL.",
  "author": "transitive-bullshit",
  "license": "MIT",
  "repository": "transitive-bullshit/react-fluid-animation",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "keywords": [
    "react",
    "fluid",
    "animation",
    "webgl",
    "gpgpu",
    "flow"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "cross-env BABEL_ENV=esm babel src -s -d dist/esm",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -s -d dist/cjs",
    "build:types": "tsc --emitDeclarationOnly",
    "prestart": "npm run clean",
    "start": "npm run build -- --watch",
    "dev": "npm start",
    "clean": "del dist",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "raf": "^3.4.1",
    "react-sizeme": "^2.5.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^18.2.0",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "^1.1.4",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ]
}
